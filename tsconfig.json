{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "./src/**/*.spec.ts"]
}
